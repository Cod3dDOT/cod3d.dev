{
	"importOrder": [
		"^(react/(.*)$)|^(react$)",
		"^(next/(.*)$)|^(next$)",
		"<THIRD_PARTY_MODULES>",
		"",
		"^@/(.*)$",
		"^[./]"
	],
	"importOrderParserPlugins": ["typescript", "jsx"],
	"plugins": [
		"@ianvs/prettier-plugin-sort-imports",
		"prettier-plugin-sort-json",
		"prettier-plugin-tailwindcss"
	],
	"printWidth": 80,
	"singleQuote": false,
	"tabWidth": 4,
	"trailingComma": "es5",
	"useTabs": true
}
